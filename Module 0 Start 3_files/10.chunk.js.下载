  if (!userId || !smwId) return []; // get active share-my-works course names

  const courseMap = (await getCourseMap()).data;
  const smwCourses = (await Object(_axios_sharemyworks__WEBPACK_IMPORTED_MODULE_1__["default"])({
    method: 'get',
    url: `/Account/${smwId}/courses`,
