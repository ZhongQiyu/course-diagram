      filter: {
        order: 'status asc',
        include: {
          relation: 'coursesDB',
          scope: {
            fields: ['curriculum', 'imageUrl', 'courseCode']
          }
        }
      }
    }
  })).data;
  console.log(courseMap);
  const activeCourses = smwCourses.filter(course => course.status !== 'Delete' && course.coursesDB && course.coursesDB.curriculum && courseMap[course.coursesDB.curriculum]).map(course => ({
    id: courseMap[course.coursesDB.curriculum].id,
    imageUrl: course.coursesDB.imageUrl,
