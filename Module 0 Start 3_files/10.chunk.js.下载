    status: course.status
  })); //console.log('active: ', activeCourses);

  const courses = await getCourses();
  const validCourses = [];
  const validCourseIds = new Set();

  for (const activeCourse of activeCourses) {
    let find = false; //console.log('active: ', activeCourse);

    for (const course of courses) {
      if (course.id === activeCourse.id && !validCourseIds.has(course.id)) {
        validCourseIds.add(course.id);
        validCourses.push({ ...course,
          image: activeCourse.imageUrl,
          courseCode: activeCourse.courseCode.toUpperCase() + ' (' + activeCourse.status + ')'
        });
        find = true;
        break;
      }
    }

    if (!find) {
      console.log('Not find course curriculum: ', activeCourse.courseCode); //console.log(activeCourse);
    }
  } //console.log(validCourses);


  return validCourses;
};

